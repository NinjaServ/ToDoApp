<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:ToDoApp.Views">

    <sys:String x:Key="aID">1</sys:String>
    <sys:String x:Key="TASK">MyTask</sys:String>
    <x:Array x:Key="MockedIDs" Type="{x:Type sys:String}">
        <sys:String>1</sys:String>
        <sys:String>2</sys:String>
        <sys:String>3</sys:String>
    </x:Array>
    <x:Array x:Key="MockedTasks" Type="{x:Type sys:String}">
        <sys:String>John</sys:String>
        <sys:String>Andy</sys:String>
        <sys:String>Tred</sys:String>
    </x:Array>
    <x:Array x:Key="MockedList" Type="{x:Type sys:String}">
        <sys:String>"MyText1"</sys:String>
        <sys:String>"MyText2"</sys:String>
        <sys:String>"MyText3"</sys:String>
    </x:Array>
</ResourceDictionary>



